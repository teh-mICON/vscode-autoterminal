{
  "name": "terminal-automation",
  "displayName": "TerminalAutomation",
  "description": "Open terminals and run commands on folder open; Automatically kill and close terminals on folder close",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.82.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/teh-mICON/vscode-terminal-automation.git"
  },
  "publisher": "techtile",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:package": "npx vsce package",
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "vscode:publish": "npx vsce publish",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.82.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-electron": "^2.3.4",
    "esbuild": "^0.19.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "express": "^4.18.2",
    "jsonc-parser": "^3.2.0"
  }
}