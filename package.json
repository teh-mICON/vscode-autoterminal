{
  "name": "terminal-automation",
  "displayName": "TerminalAutomation",
  "description": "Open terminals and run commands on folder open; Automatically kill and close terminals on folder close",
  "version": "1.0.3",
  "type": "commonjs",
  "engines": {
    "vscode": "^1.82.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/teh-mICON/vscode-terminal-automation.git"
  },
  "publisher": "techtile",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/main.js",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:package": "npx vsce package",
    "vscode:prepublish": "npx rollup -c --bundleConfigAsCjs",
    "vscode:publish": "npx vsce publish",
    "build": "rollup -c  --bundleConfigAsCjs"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.82.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "rollup": "^4.0.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "express": "^4.18.2",
    "jsonc-parser": "^3.2.0"
  }
}